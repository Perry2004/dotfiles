{
    "github.copilot.nextEditSuggestions.enabled": true,
    "window.zoomLevel": 1,
    "workbench.preferredDarkColorTheme": "Tokyo Night nv",
    "workbench.preferredLightColorTheme": "Tokyo Night Light",
    "workbench.iconTheme": "catppuccin-mocha",
    "icontheme.darkIconTheme": "catppuccin-mocha",
    "icontheme.lightIconTheme": "catppuccin-latte",
    "workbench.activityBar.location": "top",
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "Maple Mono NF CN, monospace",
    "vim.useSystemClipboard": false,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "window.autoDetectColorScheme": true,
    "vim.camelCaseMotion.enable": true,
    "vim.highlightedyank.enable": true,
    "vim.leader": " ",
    "vim.showMarksInGutter": true,
    // Lazyvim style keybindings
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "b",
                "d"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "c"
            ],
            "commands": [
                "workbench.action.closeAllEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "b"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "t"
            ],
            "commands": [
                "workbench.action.reopenClosedEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditor",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditor",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "r"
            ],
            "commands": [
                "workbench.action.closeEditorsToTheRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "l"
            ],
            "commands": [
                "workbench.action.closeEditorsToTheLeft"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "o"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "+"
            ],
            "commands": [
                "workbench.action.increaseViewHeight"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "-"
            ],
            "commands": [
                "workbench.action.decreaseViewHeight"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                ">"
            ],
            "commands": [
                "workbench.action.increaseViewWidth"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "<"
            ],
            "commands": [
                "workbench.action.decreaseViewWidth"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "d"
            ],
            "commands": [
                "workbench.action.closeEditorsInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "w"
            ],
            "commands": [
                "workbench.action.focusPreviousGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "K"
            ],
            "commands": [
                "workbench.action.moveEditorToAboveGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "_"
            ],
            "commands": [
                "workbench.action.toggleEditorWidths"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "|"
            ],
            "commands": [
                "workbench.action.toggleEditorWidths"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "d"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "b"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "p"
            ],
            "commands": [
                "workbench.action.openRecent"
            ]
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "s"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "<leader>",
                "|"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "-"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        }
    ],
    "editor.formatOnSave": true,
    "files.autoSave": "onFocusChange",
    "editor.formatOnPaste": true,
    "editor.autoIndentOnPaste": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.definitionLinkOpensInPeek": true,
    "editor.guides.bracketPairs": "active",
    "editor.linkedEditing": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.trimWhitespaceOnDelete": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "powermode.enabled": true,
    "powermode.shake.enabled": false,
    "powermode.combo.location": "off",
    "redhat.telemetry.enabled": true,
    "editor.minimap.autohide": "mouseover",
    "java.inlayHints.parameterNames.enabled": "all",
    "java.inlayHints.variableTypes.enabled": true,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "rainbow_csv.virtual_alignment_mode": "always",
    "terminal.integrated.fontLigatures.enabled": true,
    "editor.fontLigatures": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "DEBUG",
        "[TODO]",
        "[FIXME]",
        "[DEBUG]",
        "todo))",
        "fixme))",
        "[ERROR]",
        "[HACK]"
    ],
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "java.compile.nullAnalysis.mode": "automatic",
    "remote.defaultExtensionsIfInstalledLocally": [
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "GitHub.vscode-pull-request-github",
        "stackbreak.comment-divider",
        "eamodio.gitlens",
        "hoovercj.vscode-power-mode",
        "Gruntfuggly.todo-tree",
        "WakaTime.vscode-wakatime",
        "adpyke.codesnap",
        "IuriiBarlukov.cursor-column-highlight",
        "manuthebyte.dynamic-icon-theme",
        "kisstkondoros.vscode-gutter-preview"
    ],
    "[csv]": {
        "editor.inlayHints.maximumLength": 0
    },
    "terminal.integrated.scrollback": 20000,
    "terminal.integrated.persistentSessionScrollback": 500,
    "terminal.integrated.tabs.enabled": false,
}